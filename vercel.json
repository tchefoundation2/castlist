{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://farcaster.xyz https://*.farcaster.xyz https://preview.farcaster.xyz;"
        }
      ]
    }
  ]
}
