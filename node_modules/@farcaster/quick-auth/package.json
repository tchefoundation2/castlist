{
  "name": "@farcaster/quick-auth",
  "version": "0.0.6",
  "description": "Client for Farcaster Quick Auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/farcasterxyz/quick-auth.git",
    "directory": "quick-auth"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./light": {
      "types": "./dist/lightClient.d.ts",
      "default": "./dist/lightClient.js"
    },
    "./decodeJwt": {
      "types": "./dist/decodeJwt.d.ts",
      "default": "./dist/decodeJwt.js"
    }
  },
  "license": "MIT",
  "peerDependencies": {
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "jose": "^5.2.3",
    "zod": "^3.25.1"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  }
}